import java.util.Scanner;

public class Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int score = 80;
		if(90<score) {
			System.out.println("장학금 + 합격");
		}else if(60<score){
			System.out.println("합격");
		}else {
			System.out.println("재시험");
		}
		
		System.out.println("------------------------------------");
		
		Scanner input = new Scanner(System.in);
		System.out.println("숫자 0을 입력하면 모든 수와 합과 평균을 출력합니다");
		int num = input.nextInt();
		int sum = 0;
		int count = 0;
		while(num !=0 ) {
			System.out.println("숫자 0을 입력하면 모든 수와 합과 평균을 출력합니다");
			sum += num;
			num = input.nextInt();
			count++;
		}
		System.out.println("합 : "+sum+"평균 : "+sum/count);
		
		
		System.out.println("------------------------------------");
		
		int num1 = 0;
		for(int i = 0; i<5; i++) {
			System.out.println((int)Math.random()*100+"+"+(int)Math.random()*100+"=");
			if(num1 == Math.random()*100) {
				num1 = input.nextInt();
			System.out.println(Math.random()*100+"+"+Math.random()*100+"=");
		}
		}
		System.out.println("------------------------------------");
		
		// 배열

		String fu[] = {"망고", "복숭아", "자두", "포도", "사과"};
		for(int i = 0; i<fu.length; i++) {
			System.out.println(fu[i]+"가 있습니다.");
		}
		
		System.out.println("------------------------------------");
		
		int num3 = 100;
		
		for(int i = 1; i<=num3; i++) {
			if(i%3 != 0 && i%5 != 0) {
				System.out.println(i);

			}				

		}
		
		System.out.println("------------------------------------");
		
		

	}

}
