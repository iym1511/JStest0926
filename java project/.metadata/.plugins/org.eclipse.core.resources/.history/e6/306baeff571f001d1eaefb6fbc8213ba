import java.util.Scanner;

public class Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int score = 80;
		if(90<score) {
			System.out.println("장학금 + 합격");
		}else if(60<score){
			System.out.println("합격");
		}else {
			System.out.println("재시험");
		}
		
		System.out.println("------------------------------------");
		
		Scanner input = new Scanner(System.in);
		System.out.println("숫자 0을 입력하면 모든 수와 합과 평균을 출력합니다");
		int num = input.nextInt();
		int sum = 0;
		int count = 0;
		while(num !=0 ) {
			System.out.println("숫자 0을 입력하면 모든 수와 합과 평균을 출력합니다");
			sum += num;
			num = input.nextInt();
			count++;
		}
		System.out.println("합 : "+sum+"평균 : "+sum/count);
		
		
		System.out.println("------------------------------------");
		
		int num1 = 0;
		for(int i= 0; i<5; i++) {
			if() {
			num1 = input.nextInt();
			System.out.println(Math.random()*100+"+"+Math.random()*100+"="+);
			}
		}
		
		System.out.println("------------------------------------");
		
		// 배열

		String f[] = {"망고", "복숭아", "자두", "포도", "사과"};
		System.out.println(f);
	}

}
