
public class Scores {
	
	protected int korean;
	protected int math;
	protected int english;
	
	
	public Scores() {
		this.korean=0;
		this.math=0;
		this.english=0;
	}
	public Scores(int korean, int math, int english) {
		this.korean=korean;
		this.math=math;
		this.english=english;
	}

	public int arvg() {
		
		return korean+math+english/3;
	}
}
